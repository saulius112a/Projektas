@model Eshop.Data.Models.CategoryModel

@{
    ViewBag.Title = "Peržiūrėti";
}
<h2>Peržiūrėti</h2>
@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <h4>Category</h4>
        <hr />
        <div >
           <b>Kategorijos pavadinimas</b>
            <div >
                @Html.DisplayFor(model => model.Name, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            <b>Aprašymas</b>
            <div >
                @Html.DisplayFor(model => model.Description, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            <b>Kategorija kuriai priklauso</b>
            <div >
                @Html.DisplayFor(model => model.Parent.Name)

            </div>
        </div>

    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
